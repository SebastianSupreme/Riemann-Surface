class P4Vector {

  float x, y, z, w;

  public P4Vector(float x, float y, float z, float w) {

    this.x = x;
    this.y = y;
    this.z = z;
    this.w = w;
  }

  public P4Vector(float x, float y, float z) {

    this.x = x;
    this.y = y;
    this.z = z;
  }

  public P4Vector(float x, float y) {

    this.x = x;
    this.y = y;
  }
}
